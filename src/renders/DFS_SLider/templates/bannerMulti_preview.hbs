                <img src="https://{{image.defaultHost}}/i/dfs/?layer0=[canvas=1366,619]&layer1=[src=/i/dfs/{{productImage.name}}&{{#if desktopMImageCrop}}{{desktopMImageCrop}}&{{/if}}{{#if productImageFullWidth}}w=1366&h=619&anchor=l&left=0{{else}}w=1024&h=619&anchor=l&left=342{{/if}}]{{#if image.name}}&layer2=[/i/dfs/{{image.name}}&left=0&h=100%]{{/if}}{{#if sidebarCharacter.name}}&layer3=[/i/dfs/{{sidebarCharacter.name}}&{{desktopCharacterCrop}}&{{desktopCharacterSize}}&bottom={{sideBarCBottom}}&left={{sideBarCLeft}}&anchor=bl]{{/if}}{{#if pricingImage.name}}&layer4=[src=/i/dfs/{{#if ../spec.roi }}ROI_{{/if}}{{pricingImage.name}}&anchor=br&right=24&bottom=24]{{/if}}{{#if EspotImage.name}}&layer5=[src=/i/dfs/{{#if ../spec.roi }}ROI_{{/if}}{{EspotImage.name}}&anchor=tr&right={{EspotImage_right}}&top={{EspotImage_top}}]{{/if}}&qlt=70&fmt=jpg&fmt.jpeg.chroma=1,1,1">
              </picture>

              <div class="bannerTextContainer {{countdownClassName}}{{#if hasCountdown}} carCountdown{{/if}}" data-messageid="slide_{{inc @index}}">
                <!-- START:: V2 CountDown (PB) July 2nd 2018-->
                  <div class="countdownContainer countdown_v2 {{countdownClassName}} {{#if hasCountdown}} {{else}} nonCDText{{/if}}"
                        data-deadline="{{countdownDeadline}}"
                        data-startdays="{{daysBeforeEnd}}"
                        data-daterange1="{{textVariance1}}"
                        data-daterange2="{{textVariance2}}"
                        data-daterange3="{{textVariance3}}"
                        data-daterange4="{{textVariance4}}"
                        data-hidefinalmessage="{{#if hideFinalMessage}}true{{else}}false{{/if}}"
                        data-testdate="{{../../testDate}}">
                    <h6 class="cdHeader"><!-- DEFAULT HEADER TEXT--></h6>
                      <section class="timer {{#if hasCountdown}} showCountdown{{else}}hideCountdown{{/if}}">
                        {{!-- <img src="//dfs.a.bigcontent.io/v1/static/loading_please_wait?v30" alt="loading.."> --}}
                      </section>
                    <h6 class="cdFooter"><!-- DEFAULT FOOTER TEXT--></h6>
                  </div>
                  <div style="display:none;" class="cdEndMessage {{countdownClassName}}">
                    <h3>ENDS TODAY<br>Order direct until midnight</h3>
                  </div>
                <!-- END:: V2 CountDown (PB) July 2nd 2018-->
              </div>
            </a>
            {{#iff carouselVersion "==" "Late2019" }}
            
            <div class="availability">
              {{#if showIcons }}
              <div class="colour">
                <p><img src="https://images.dfs.co.uk/i/dfs/lister_colourwheel .png?w=20">More colours available</p>
              </div>
              <div class="sizes">
                <p><img src="https://images.dfs.co.uk/i/dfs/Lister-sizes">More sizes <span>available</span></p>
              </div>
              <div class="clearfix"></div>
              {{/if}}
            </div>
            
            <div class="productDetail _mobile_ {{priceLabelStyle}}">
                    <h4 class="product_title">{{{_title}}}</h4>
                    <h5 class="offer_price">{{priceLabel.[0]}} {{priceLabel.[1]}}</h5>
                    <p class="additional_pricing_details">{{#iff priceLabel.[4] "==" "none" }}{{priceLabel.[2]}} <span>{{priceLabel.[3]}}</span> {{priceLabel.[7]}}{{else}}{{priceLabel.[4]}} {{priceLabel.[2]}} <span>{{priceLabel.[3]}}</span>{{/iff}}</p>
                    <div class="carousel_cardlist_button">
                      <a href="{{shopButtonUrl}}">
                        <p style="color:#000;border: 2px solid #000">{{shopButtonText}}</p>
                      </a>
                    </div>
                </div>

                <div class="productDetail _label_ {{priceLabelStyle}}">
                  <div class="label_left">
                    <h5 class="offer_price">{{priceLabel.[0]}}<span>{{priceLabel.[1]}}</span>{{priceLabel.[3]}}</h5>
                  </div>
                  
                  <div class="label_middle ">
                    {{#iff priceLabel.[4] "==" "none" }}
