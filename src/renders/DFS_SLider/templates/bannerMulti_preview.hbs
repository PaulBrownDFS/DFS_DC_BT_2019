                    {{/iff}}
                  </div>

                  <div class="label_right_finance">
                    <p class="additional_pricing_details"><span>{{financeArray.[0]}}</span>{{financeArray.[2]}}<span></span>{{financeArray.[1]}}</p>
                  </div>
                
                  {{#if priceLabel.[5]}}
                    <div class="available_colours">
                      <img src="https://images.dfs.co.uk/i/dfs/{{priceLabel.[5]}}/colour_swatches?qlt=100" alt="{{priceLabel.[5]}}">
                    </div>
                    </div>
